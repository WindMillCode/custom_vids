{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "hosting":{
      "enabled":false
    },
    "storage": {
      "port": 9198
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
